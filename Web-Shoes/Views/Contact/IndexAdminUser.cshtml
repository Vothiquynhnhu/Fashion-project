@model IEnumerable<Web_Fashion.Entity.ContactUsers>

@{
    ViewData["Title"] = "IndexAdminUser";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}



<div id="page-wrapper">
    <div id="page-inner">
        <div class="row">
            <div class="col-lg-12">
                <h2>Users:</h2>
            </div>
        </div>
<hr />
    <div class="row">
        <div class="col-lg-12 ">

            <table class="table">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.cu_FirstName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.cu_LastName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.cu_Email)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.cu_Subject)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.cu_Description)
                        </th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.cu_FirstName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.cu_LastName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.cu_Email)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.cu_Subject)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.cu_Description)
                            </td>
                            <td>
                                    <a asp-action="Details" asp-route-id="@item.cu_Id">Details</a> |
                                <a asp-action="Delete" asp-route-id="@item.cu_Id">Delete</a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>

        </div>
    </div>
</div>
</div>


